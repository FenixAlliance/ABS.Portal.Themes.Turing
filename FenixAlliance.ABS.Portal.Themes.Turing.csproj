    <Description>Application Component for the Alliance Business Suite.</Description>
    <RepositoryUrl>https://github.com/FenixAlliance/ABS.Portal.UI</RepositoryUrl>
    <StaticWebAssetBasePath Condition="$(StaticWebAssetBasePath) == ''">/ui</StaticWebAssetBasePath>
  </PropertyGroup>
  <ItemGroup>
    <Content Remove="comps\**" />
    <Compile Remove="comps\**" />
    <EmbeddedResource Remove="comps\**" />
    <None Remove="comps\**" />
  </ItemGroup>
  <ItemGroup>
    <!--<PackageReference Include="FenixAlliance.ABS.Core" Version="1.2.0" />-->

    <ProjectReference Include="..\..\..\..\FenixAlliance.ABS\FenixAlliance.ABS.Core\FenixAlliance.ABS.Core.csproj" />
